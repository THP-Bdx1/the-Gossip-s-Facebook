<h1>Déjà inscrit ?</h1>
<p>File tes pass mec !</p>

<%= form_for(:registration, url: login_path) do |f| %>
<div class="form-group">
    <%= f.text_field :email, placeholder: 'Email', class: "form-control" %>
</div>
<div class="form-group">
    <%= f.text_field :password, placeholder: 'Mot de passe', class: "form-control" %>
</div>
<div class="form-group">
    <button class="btn btn-primary" type="submit">Envoyer</button>
</div>
<% end %>
<div class="center jumbotron">
    <%= link_to "Nouvel utilisateur ?", new_user_path, class: "btn btn-lg btn-primary" %>
</div>